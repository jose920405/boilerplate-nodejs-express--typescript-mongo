{
  "name": "node-boilerplate",
  "version": "0.0.1",
  "description": "nodejs with express boilerplate",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "tsc --noEmit && tslint -c tslint.json -p tsconfig.json '**/*.ts{,x}' -e '**/node_modules/**'",
    "start-dev": "ts-node-dev --respawn --transpileOnly ./server.ts",
    "prod": "tsc && node ./build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "node",
    "nodejs",
    "server",
    "boilerplate",
    "nodebase"
  ],
  "author": "Jose Omar Gonzalez",
  "license": "MIT",
  "dependencies": {
    "await-to-js": "2.1.1",
    "body-parse": "0.1.0",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "mongoose": "5.7.1",
    "ts-node-dev": "1.0.0-pre.42",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "@types/express": "4.17.1",
    "@types/lodash": "4.14.138",
    "@types/mongoose": "5.5.17"
  }
}